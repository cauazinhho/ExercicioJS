//Faça um programa que imprima os números ímpares de 0 a 50;

for(let contador=0; contador <=50; contador++){

    
    if (contador  % 2 != 0) {
        console.log(contador)
    }

}

for(let contador=0; contador <=50; contador++){

    //"!=="" logica inversa, diferente de 0
    if (contador % 2 !==0) {
        console.log(contador)
    }

}
